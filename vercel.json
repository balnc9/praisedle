{
  "version": 2,
  "name": "praisedle",
  "routes": [
    {
      "src": "/(.*\\.(css|js|json|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/",
      "dest": "/praisedle.html"
    },
    {
      "src": "/(.*)",
      "dest": "/praisedle.html"
    }
  ]
} 